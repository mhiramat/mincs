
# enable ftrace related configs with tests
kconfigs_y FUNCTION_TRACER FUNCTION_GRAPH_TRACER STACK_TRACER
kconfigs_y KPROBES KPROBE_EVENTS 
kconfigs_y FTRACE \
	PREEMPTIRQ_EVENTS FTRACE_SYSCALLS TRACER_SNAPSHOT \
	MMIOTRACE FUNCTION_PROFILER
kconfigs_y HIST_TRIGGERS
kconfigs_y IRQSOFF_TRACER PREEMPT_TRACER SCHED_TRACER HWLAT_TRACER
kconfigs_y BOOTTIME_TRACING BOOT_CONFIG
kconfigs_y WATCHPOINT_EVENTS
kconfigs_y TRACE_EVENT_INJECT
kconfigs_y TRACE_OBJECT
kconfigs_y UPROBES UPROBE_EVENTS
kconfigs_y DYNAMIC_FTRACE
kconfigs_y KPROBE_EVENTS_ON_NOTRACE
kconfig_set SAMPLE_TRACE_EVENTS m
kconfig_set PREEMPTIRQ_DELAY_TEST m
