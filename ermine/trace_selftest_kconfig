# Note that this will slow down the boot time very much.
  kconfigs_y KUNIT KPROBES_SANITY_TEST FPROBE_SANITY_TEST
  kconfigs_y FTRACE_STARTUP_TEST EVENT_TRACE_STARTUP_TEST EVENT_TRACE_TEST_SYSCALLS
  kconfigs_y FTRACE_SORT_STARTUP_TEST RING_BUFFER_STARTUP_TEST
  kconfigs_y RING_BUFFER_VALIDATE_TIME_DELTAS
  kconfigs_m MMIOTRACE_TEST PREEMPTIRQ_DELAY_TEST
  kconfigs_y SYNTH_EVENT_GEN_TEST KPROBE_EVENT_GEN_TEST
